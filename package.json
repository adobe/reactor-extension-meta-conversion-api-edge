{
  "name": "@adobe/reactor-extension-meta-conversion-api-edge",
  "platform": "edge",
  "version": "1.4.0",
  "description": "Send web events to Meta's Conversions API.",
  "author": {
    "name": "Adobe",
    "url": "http://adobe.com",
    "email": "reactor@adobe.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe/reactor-extension-meta-conversion-api-edge.git"
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "clean": "rm -rf dist",
    "sandbox": "concurrently -k -n webpack,sandbox -c \"bgBlue,bgMagenta\" \"npm run build:watch\" \"reactor-sandbox\"",
    "package": "npm run clean && npm run build && npx @adobe/reactor-packager",
    "prepack": "npm run clean && npm run build",
    "lint": "eslint 'src/**'",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "update": "rm -rf node_modules package-lock.json && npm i",
    "prepare": "husky install"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.30.0",
    "@spectrum-icons/illustrations": "^3.6.5",
    "@spectrum-icons/workflow": "^4.2.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.2",
    "react-stately": "^3.26.0"
  },
  "devDependencies": {
    "@adobe/reactor-sandbox": "^12.5.0",
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-import-assertions": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/runtime": "^7.23.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "camelcase": "^8.0.0",
    "capitalize": "^2.0.4",
    "concurrently": "^8.2.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.0.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-runner-eslint": "^2.1.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "stylus": "^0.59.0",
    "stylus-loader": "^7.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "yargs": "^17.7.2"
  }
}
